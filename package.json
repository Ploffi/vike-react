{
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cd vike-react/ && pnpm run dev",
    "build": "pnpm --recursive --filter {packages/*} run build",
    "========= Clean": "",
    "clean": "git clean -Xdf",
    "reset": "pnpm run clean && pnpm install && pnpm run build",
    "========= Release": "",
    "vike-react:release": "cd ./packages/vike-react/ && pnpm run release",
    "vike-react:release:commit": "cd ./packages/vike-react/ && pnpm run release:commit",
    "========= Only allow pnpm; forbid yarn & npm": "",
    "preinstall": "npx only-allow pnpm"
  },
  "packageManager": "pnpm@8.6.12",
  "devDependencies": {
    "prettier": "^3.1.0"
  }
}